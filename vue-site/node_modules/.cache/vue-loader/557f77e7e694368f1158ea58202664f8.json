.get(this.$client.apiUrlprefix() + \"/webapiscraper/result\", {}, function(res) {\r\n\t\t\t\tconsole.log(res)\r\n\t\t\t\tif (res) {\r\n\t\t\t\t\tif (res.data.retcode == 0) {\r\n\t\t\t\t\t\tconsole.log(res.data.result.cases)\r\n\t\t\t\t\t\t_that.OnResponse(res.data.result.cases)\r\n\t\t\t\t\t\treturn true;\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\treturn false;\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t})\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n\t.el-header,\r\n\t.el-footer {\r\n\t\tbackground-color: #b3c0d1;\r\n\t\tcolor: #333;\r\n\t\ttext-align: center;\r\n\t\tline-height: 200px;\r\n\t}\r\n</style>\r\n"]}]}